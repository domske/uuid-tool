{
  "name": "uuid-tool",
  "version": "2.0.1",
  "description": "Lightweight UUID for JavaScript. Parse and generate UUIDs. Convert between string and byte array.",
  "keywords": [
    "uuid",
    "parse",
    "generate",
    "compare",
    "byte",
    "array",
    "node",
    "browser",
    "guid"
  ],
  "author": {
    "name": "Dominik-Andreas Geng",
    "url": "https://github.com/domske/"
  },
  "homepage": "https://domske.github.io/uuid-tool/",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint  ./src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/domske/uuid-tool"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.3",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
